<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.mohan.appp.Malefemale"
    tools:showIn="@layout/activity_malefemale">

    <ImageView
        android:id="@+id/femimg"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/fem"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"/>
    <ImageView
        android:id="@+id/maleimg"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/male"
        android:layout_toRightOf="@id/femimg"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="50dp"/>
    <Button
        android:id="@+id/gender"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/femimg"
        android:text="NEXT"
        android:layout_marginTop="40dp"/>


</RelativeLayout>
